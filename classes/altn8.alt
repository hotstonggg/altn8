{
    "JAVA" => [
        [/^Test(.*)$/, '\1'],
        [/^(.*)$/, 'Test\1']
    ],
    # TODO Would be nice to support more than one output, i.e. for sproc to ruby / xml tests.
    "PLAIN_TEXT" => [
        [/^test_(.*)\.rb$/, '\1.sp'],
        [/^(.*)\.sp$/, 'test_\1.xml'],
        [/^(.*)\.sp$/, 'test_\1.rb']
    ],
    "XML" => [
        [/^test_(.*)\.xml$/, '\1.sp']
    ]
}